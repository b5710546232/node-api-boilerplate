{
  "name": "node-api-boilerplate",
  "version": "1.0.0",
  "description": "This is a simple boilerplate for building API with Node.",
  "main": "build/bin/server.js",
  "scripts": {
    "eslint": "eslint --quiet **/**/*.js **/*.js **.js",
    "dev": "babel-node ./src/bin/server",
