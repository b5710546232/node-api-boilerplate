    "url": "https://github.com/zugarzeeker/node-api-boilerplate/issues"
  },
  "homepage": "https://github.com/zugarzeeker/node-api-boilerplate#readme",
  "dependencies": {
    "babel-eslint": "^6.1.2",
    "body-parser": "^1.15.2",
    "cloudinary": "^1.4.1",
    "compression": "^1.6.2",
    "config": "^1.21.0",
    "connect-mongo": "^1.3.2",
    "cors": "^2.7.1",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-flash": "0.0.2",
    "express-session": "^1.14.0",
    "express-validator": "^2.20.8",
